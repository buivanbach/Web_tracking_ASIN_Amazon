{
  "name": "web-tracking-asin-amazon",
  "version": "1.0.0",
  "description": "Web application for tracking Amazon product ASINs and rankings with automated crawling",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:prod": "node server-production.js",
    "dev": "nodemon server.js",
    "crawl": "python python/crawl_and_update_fixed.py",
    "test-setup": "node test-setup.js",
    "run": "node scripts/run/run.js",
    "setup": "powershell -ExecutionPolicy Bypass -File setup.ps1",
    "run:win": "powershell -ExecutionPolicy Bypass -File run.ps1",
    "setup:firewall": "sudo chmod +x setup-firewall.sh && ./setup-firewall.sh",
    "aws:deploy": "sudo chmod +x aws-auto-deploy.sh && ./aws-auto-deploy.sh",
    "aws:manage": "chmod +x manage-service.sh && ./manage-service.sh",
    "aws:auto-restart": "sudo chmod +x setup-auto-restart.sh && ./setup-auto-restart.sh",
    "aws:monitor": "chmod +x monitor.sh && ./monitor.sh"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "web-ranking",
    "crawler",
    "mvc",
    "express",
    "sqlite"
  ],
  "author": "Your Name",
  "license": "MIT"
}
